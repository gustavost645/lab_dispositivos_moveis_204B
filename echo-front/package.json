    "@types/jasmine": "~4.3.0",
    "@types/papaparse": "^5.3.15",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "sass": "^1.81.0",
    "typescript": "~5.1.3"
  }
