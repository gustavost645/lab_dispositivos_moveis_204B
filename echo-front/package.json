{
  "name": "echo-front",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.js --configuration development",
    "start-prod": "ng serve --proxy-config proxy.conf.js --configuration production",
    "build": "ng build ",
    "watch": "ng build --watch --configuration development",
    "build-prod": "npm run clean-prod && ng build --configuration production --output-path=dist/prod",
    "clean-prod": "rimraf dist/prod",
    "lint": "ng lint",
    "test": "ng test --watch=false",
    "build-full": "npm run lint && npm run test && npm run build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.7",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.7",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "echo-front": "file:",
    "echoo": "file:",
    "firebase": "^11.0.1",
    "ngx-mask": "^18.0.4",
    "papaparse": "^5.4.1",
    "pedidos": "file:",
    "primeflex": "^3.3.1",
    "primeicons": "6.0.1",
    "primeng": "^16.3.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.3",
    "@angular/cli": "~16.2.3",
